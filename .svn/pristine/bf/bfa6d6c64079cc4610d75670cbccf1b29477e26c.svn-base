/******************************************************************************
 *                                                                            *
 *                                   GENERAL                                  *
 *                                                                            *
 ******************************************************************************/

/* Font for everything on the page */

body
{
	overflow: scroll-y;
}

body, td, button, input, textarea
{
    font: 12px/15px Arial, sans-serif;
}

/* Headings */

h1, h2
{
	color: #1f1f1f;
	margin: 0;
	font-size: 15px;
}

h1
{
	font-weight: bold;
	line-height: 20px;
}

h1.right
{
	float: right;
}

h1.section, .wrapper .wrapper h1.section
{
	color: #262626;
	border-bottom: 1px solid #c0c0c0;
	margin: 10px 0;
	padding: 0 0 0 5px;
}

h1.section .tag
{
	border-left: 1px solid #c0c0c0;
	float: right;
	font-size: 11px;
	line-height: 14px;
	margin: 2px 0 0 10px;
	padding: 2px 0 2px 10px;
}

h1.section .tag:first-child
{
	border-right: 1px solid #c0c0c0;
	padding-right: 10px;
}

h1 label
{
	color: #6f6f6f;
}

h1 span
{
	font-weight: normal;
}

.wrapper .wrapper h1
{
	margin: 0 5px;
}

h2
{
	font-weight: normal;
	line-height: 18px;
}

/* Disabled state */

input[type=number][disabled], input[type=text][disabled], input.text[disabled], textarea[disabled], .autoCompleteContainer .searchField[readonly][disabled]
{
	-moz-box-shadow: none;
	-webkit-box-shadow: none;
	background: #F2F2F2;
	box-shadow: none;
	color: #999999;
}

.boxedField .value table td input[type=text][disabled]
{
	background: #F2F2F2;
}


/******************************************************************************
 *                                                                            *
 *                                  TEXTBOXES                                 *
 *                                                                            *
 ******************************************************************************/

input[type=number], input[type=text], input[type=password]
{
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	background: #fff;
	border: 1px solid #afafaf;
	border-top-color: #7f7f7f;
	border-bottom-color: #bfbfbf;
	border-right-color: #a7a7a7;
	-moz-box-shadow: inset 0 1px 0 rgba(0, 0, 0, 0.1), inset 1px 0 0 rgba(0, 0, 0, 0.1);
	-webkit-box-shadow: inset 0 1px 0 rgba(0, 0, 0, 0.1), inset 1px 0 0 rgba(0, 0, 0, 0.1);
	box-shadow: inset 0 1px 0 rgba(0, 0, 0, 0.1), inset 1px 0 0 rgba(0, 0, 0, 0.1);
	box-sizing: border-box;
	padding: 3px 4px 4px;
	font-size: 12px;
	/*height: 24px;*/
	line-height: 15px;
	margin: 0;
}

:root input[type=number], :root input[type=text], :root input[type=password]
{
	padding: 4px\9; /* Targets IE9+ */
}

.mac input[type=number], .mac input[type=text], .mac input[type=password]
{
	padding: 4px 4px 3px;
}

/******************************************************************************
 *                                                                            *
 *                                    TABS                                    *
 *                                                                            *
 ******************************************************************************/

/* Optional tab set heading */

h1.tab
{
	color: #8bad3f;
	cursor: default;
	float: left;
	padding: 13px 0 0 10px;
	width: 265px;
}

.mac h1.tab
{
	width: 282px;
}

@media screen and (-webkit-min-device-pixel-ratio:0)
{
	.mac h1.tab
	{
		width: 280px;
	}
}

/* Optional surrounding panel for tabs */

.tabSetPanel
{
    background: #F7F7F7;
}

.tabSetPanel, h1.tab
{
	-moz-user-select: none;
	-webkit-user-select: none;
	user-select: none;
}

.tabSetPanel > div
{
    background: #fff;
}

.unselectable
{
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-khtml-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

.tabSetPanel .dropdown
{
	float: right;
	margin: 0 10px 0 0;
	padding: 15px 0 0 0;
}

/* General style for tabs */

ul.tabSet
{
    -moz-user-select: none;
    -webkit-user-select: none;
    border-bottom: 1px solid #bfbfbf;
    list-style: none;
    margin: 0;
    padding: 11px 0 0 10px;
    user-select: none;
}

.tabSetPanel ul.tabSet
{
    padding: 16px 0 0 5px;
}

ul.tabSet h1
{
	float: left;
	left: 5px;
	position: relative;
	height: 25px;
}

ul.tabSet br
{
	clear: left;
}

/* Tab content */

.tabSetPanel .tabSet
{
	#overflow: visible;
}

/* note checkBox */
.noteCheckBox
{
	position: absolute;
    top: 12px;
    color:#fff;
}


