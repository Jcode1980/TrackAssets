.auto_complete
{
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
	border-color: red #afafaf #7f7f7f #a7a7a7;
	border-top: 0;
	box-sizing: border-box;
	overflow: auto;
	max-height: 300px;
	margin-bottom: auto; /* This is important! Fixes an IE9 bug */
