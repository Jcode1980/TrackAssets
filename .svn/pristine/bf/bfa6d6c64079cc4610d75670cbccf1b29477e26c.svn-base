/******************************************************************************
 *                                                                            *
 *                                  DROPDOWNS                                 *
 *                                                                            *
 ******************************************************************************/

/* Normal dropdown */

.dropdown
{
	-moz-user-select: none;
	-webkit-user-select: none;
    margin-bottom: 10px;
    user-select: none;
    width: 220px;
}

.dropdown, .dropdown a
{
    cursor: default;
}

.dropdown .caption
{
	background: #efefef;
    border: 1px solid #7f7f7f;
    display: block;
    height: 18px;
    line-height: 18px;
    padding: 2px 6px;
}

.dropdown .caption:hover
{
	background: #fff;
}

.dropdown .caption .value
{
	font-weight: bold;
}

.dropdown .arrow
{
    float: right;
    vertical-align: middle;
    margin-left: 5px;
}

.dropdown .arrow span
{
	background-image: url(<webobject name="ResourceURLDropDownArrowBlack"></webobject>);
	background-repeat: no-repeat;
    display: inline-block;
    height: 5px;
    position: relative;
    top: -1px;
    vertical-align: middle;
    width: 9px;
}

.dropdown ul
{
	display: none;
}

.dropdown .open ul
{
    -moz-box-shadow: 0 4px 4px rgba(0, 0, 0, 0.2);
    -webkit-box-shadow: 0 4px 4px rgba(0, 0, 0, 0.2);
    border: 1px solid #cfcfcf\9;
    border-top: 0;
    box-shadow: 0 4px 4px rgba(0, 0, 0, 0.2);
    display: block;
    list-style: none;
    margin: 0;
    padding: 0;
    position: absolute;
    z-index: 9;
}

.dropdown .open .caption
{
    -moz-box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);
    -webkit-box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);
    background: #fff;
    border-color: #fff;
    box-shadow: 0 0 5px rgba(0, 0, 0, 0.3);
    border-color: #cfcfcf\9;
    border-bottom: 0\9;
    padding-bottom: 3px\9;
}

.dropdown .open .arrow span
{
	background-image: url(<webobject name="ResourceURLDropDownBarBlack"></webobject>);
}

.dropdown .divider
{
    background: #fff;
    cursor: default;
    display: block;
    height: 1px;
    padding: 0 1px;
}

.dropdown .divider span
{
    background: #e7e7e7;
    display: block;
    height: 1px;
}

.dropdown li a
{
    background: #fff;
    color: #000;
    cursor: default;
    display: block;
    line-height: 18px;
    padding: 2px 7px 4px;
    text-decoration: none;
    width: 206px;
    width: 204px\9;
}

.dropdown .selected a
{
	font-weight: bold;
}

.dropdown li a:hover
{
	background: #def2b6;
}

.dropdown .disabled a, .dropdown .disabled a:hover
{
	color: #7f7f7f;
	background: #efefef;
}

.dropdown .disabled a .iconMedium
{
	filter: alpha(opacity=50);
	opacity: .50;
}

.dropdown .disabled.tip a
{
	color: #000;
	font-style: italic;
}

.dropdown .iconMedium
{
	top: -1px;
}

/* Batch bar dropdown variant */

.batchBar .dropdown .caption
{
    -moz-box-shadow: none;
    -webkit-box-shadow: none;
	background: #739f40;
	border-color: #62933f;
	box-shadow: none;
	color: #fff;
	text-align: right;
}

