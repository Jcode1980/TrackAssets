.modal.window
{
	-moz-transition: opacity 0.5s;
	-webkit-transition: opacity 0.5s;
	-o-transition: opacity 0.5s;
	background: #fff;
	display: none;
	height: auto;
	position: relative;
	text-align: left;
	transition: opacity 0.5s;
}

.modal.window.loading
{
	-moz-transition: opacity 0.5s;
	-webkit-transition: opacity 0.5s;
	-o-transition: opacity 0.5s;
	filter: alpha(opacity=0);
	transition: opacity 0.5s;
	opacity: 0;
}

.modalFloater .modal.window
{
	display: inline-block;
	vertical-align: middle;
}

.modalBarrier, .modalFloater, .modal.window .overlay > table
{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	_position: absolute;
	_left: expression( _ = document.documentElement.scrollLeft - document.body.offsetLeft + 'px' );
	_top: expression( _ = document.documentElement.scrollTop - document.body.offsetTop + 'px' );
	_width: expression( document.documentElement.clientWidth + "px" );
	_height: expression( document.documentElement.clientHeight + "px" );
}

.modalBarrier
{
	background: #000;
	opacity: 0;
	filter: alpha(opacity=0);
	z-index: 998;
}

